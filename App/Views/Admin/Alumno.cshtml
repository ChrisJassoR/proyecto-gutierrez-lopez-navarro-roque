@using App.Models.ViewModels
@using App.Models.DataBaseModel
@model AlumnoViewModel
@foreach (var alumno in Model.Alumnos )
{
    <div>
        <h5>
            @alumno.alumnoId 
            @alumno.nombre
        </h5>
    </div>
}

<div>
    <h1>Buscar Alumno</h1>
    @await Html.PartialAsync("Partials/Alumno/BuscarAlumnoPartial")
</div>
<div>
    <h1>Agregar Alumno</h1>
    @await Html.PartialAsync("Partials/Alumno/AgregarAlumnoPartial", new Alumno())
</div>
<div>
    <h1>Elimininar Alumno</h1>
    @await Html.PartialAsync("Partials/Alumno/EliminarAlumnoPartial")
</div>
<div>
    <h1>Modificar Alumno</h1>
    @await Html.PartialAsync("Partials/Alumno/ModificarAlumnoPartial")
</div>